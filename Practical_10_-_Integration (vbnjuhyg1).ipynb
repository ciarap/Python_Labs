   "cell_type": "code",
   "execution_count": 38,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "# define symbols\n",
    "x=Symbol('x')\n",
    "f= -x**2 -2*x +2\n",
    "g= -x**2+6\n"
   ]
